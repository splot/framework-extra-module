{
    "name": "splot/framework-extra-module",
    "description": "Extra features for Splot Framework.",
    "keywords": ["ajax", "databridge", "filestorage", "form", "imaging", "mailer", "process"],
    "license": "MIT",
    "authors": [
        {
            "name": "Michał Pałys-Dudek",
            "email": "michal@michaldudek.pl"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "psr/log": "1.0.*",
        "swiftmailer/swiftmailer": ">=5.1,<6.0",
        "gregwar/image": "~2.0",
        "michaldudek/foundation": ">=0.9,<2.0",
        "splot/framework": ">=0.9,<2.0",
        "splot/twig-module": ">=0.9,<2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "extra": {
        "branch-alias": {
            "dev-release/0.9-alpha": "0.9.x-dev"
        }
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": { 
            "Splot\\FrameworkExtraModule\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Splot\\FrameworkExtraModule\\Tests\\": "tests/"
        }
    }
}
